   <children>
      <TextField fx:id="nameField" layoutX="191.0" layoutY="24.0" prefHeight="25.0" prefWidth="174.0" promptText="Event Name" />
      <DatePicker fx:id="startDateDatePicker" layoutX="193.0" layoutY="71.0" promptText="Start Date Of The Event" />
      <DatePicker fx:id="endDateDatePicker" layoutX="191.0" layoutY="120.0" promptText="End Date Of The Event" />
      <TextField fx:id="descriptionTextField" layoutX="191.0" layoutY="175.0" prefHeight="25.0" prefWidth="174.0" promptText="Event's Description" />
      <TextField fx:id="locationTextField" layoutX="193.0" layoutY="226.0" prefHeight="25.0" prefWidth="174.0" promptText="Location Of The Event" />
      <Button fx:id="addButton" layoutX="191.0" layoutY="283.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="25.0" prefWidth="174.0" text="Add Event" />
   </children>
</AnchorPane>
