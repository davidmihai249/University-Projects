<AnchorPane prefHeight="271.0" prefWidth="474.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkgui.controller.EditFriendRequestController">
    <GridPane layoutX="86.0" layoutY="73.0" prefHeight="91.0" prefWidth="324.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="158.0" minWidth="10.0" prefWidth="124.0" />
